<tr>
  <td><%= index_for user_counter , @users.current_page,
    Settings.item_per_page %></td>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.created_at.to_date %></td>
  <td>
    <%= link_to edit_admin_user_path user do %>
      <%= fa_icon "pencil" %>
    <% end %>
  </td>
</tr>
